  "scripts": {
    "devmode": "./scripts/windows/devmode_build.bat",
    "devmode:windows": ".\\scripts\\windows\\devmode_build.bat",
    "build": "tsc --build --clean && tsc --build && npx babel ./src --out-dir ./build --extensions .ts,.tsx --copy-files",
    "test": "jest --passWithNoTests",
    "run-example": "./scripts/windows/setup_and_run_example.bat",
    "run-example:windows": ".\\scripts\\windows\\setup_and_run_example.bat",
    "lint": "eslint src/**/*.{js,ts,tsx}",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json}\" && prettier --write \"docs/*\"",
    "release": "semantic-release"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "iav-frontend-framework"
  ],
  "dependencies": {
    "i18next": "^22.4.13",
    "i18next-browser-languagedetector": "^7.0.1",
    "primeflex": "^3.3.0",
    "primeicons": "^6.0.1",
    "primereact": "^10.3.3",
    "react-cookie": "^7.2.1",
    "react-i18next": "^12.2.0",
    "simplebar-react": "^3.2.3"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.9.0",
    "react-router-dom": "^6.9.0",
    "react-transition-group": "^4.4.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.22.10",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.21.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.3.3",
    "@svgr/babel-plugin-transform-svg-component": "^8.0.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.15.5",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "aws-amplify": "^6.3.4",
    "babel-plugin-inline-react-svg": "^2.0.2",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "semantic-release": "^24.1.0",
    "typescript": "^5.0.2"
  },
  "files": [
    "./LICENSE",
    "./NOTICE",
    "./package.json",
    "./README.md",
    "./build"

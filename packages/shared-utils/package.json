{
  "name": "@iavofficial/frontend-framework-shared-utils",
  "version": "1.0.0",
  "private": false,
  "license": "Apache-2.0",
  "description": "This package provides utility methods of use in multiple frontend framework packages.",
  "scripts": {
    "setup": "npm i",
    "build": "npx babel ./src --out-dir ./dist --extensions .ts,.tsx --copy-files && npx babel ./package.json --out-dir ./dist --copy-files && npx tsc --outDir ./dist",
    "build-live": "cross-env-shell \"..\\..\\scripts\\win\\build-live.bat || echo \"Not implemented yet.\"",
    "test": "jest --passWithNoTests",
    "lint": "eslint src/**/*.{js,ts,tsx}",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json}\" && prettier --write \"docs/*\"",
    "release": "npx semantic-release",
    "generate-docs": "../../scripts/unix/generate-docs-for-package.sh"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "iav-frontendframework",
    "aws authenticator"
  ],
  "dependencies": {
    "@reduxjs/toolkit": "^2.5.1",
    "aws-amplify": "^6.3.4",
    "react-redux": "^9.2.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.22.10",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.21.0",
    "@svgr/babel-plugin-transform-svg-component": "^8.0.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.15.5",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.3.4",
    "babel-plugin-inline-react-svg": "^2.0.2",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "typescript": "^5.0.2"
  },
  "files": [
    "./package.json",
    "./dist"
  ],
  "exports": {
    "./containsOneOrMoreGroups": "./dist/auth/containsOneOrMoreGroups.js",
    "./parseLanguageResourcesIntoDropdownFormat": "./dist/internationalization/parseLanguageResourcesIntoDropdownFormat.js",
    "./hash": "./dist/hash.js"
  },
  "typesVersions": {
    "*": {
      "./containsOneOrMoreGroups": [
        "./dist/auth/containsOneOrMoreGroups.d.ts"
      ],
      "./parseLanguageResourcesIntoDropdownFormat": [
        "./dist/internationalization/parseLanguageResourcesIntoDropdownFormat.d.ts"
      ],
      "./hash": [
        "./dist/hash.d.ts"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
